    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jwks-rsa": "^3.1.0",
    "lru-cache": "^11.0.0",
    "mssql": "^10.0.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.10.0",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.5",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
